{
	"name" : "slothsoft/farah",
	"description" : "Files and Resources and Hypertext: A content management system.",
	"license" : "MIT",
	"authors" : [{
			"name" : "Daniel Schulz",
			"email" : "info.slothsoft@gmail.com"
		}
	],
	"bin" : [
		"scripts/farah-asset",
		"scripts/farah-page"
	],
	"type" : "library",
	"require" : {
		"php" : ">=7.4",
		"php-ds/php-ds" : "^1.2",
		"ext-sockets" : "*",
		"guzzlehttp/psr7" : "^2.7",
		"matthiasmullie/minify" : "^1.3",
		"psr/cache" : "^1.0",
		"psr/container" : "^1.0",
		"psr/http-message" : "^1.0",
		"psr/log" : "^1.0",
		"slothsoft/blob" : "^1.0",
		"slothsoft/core" : "^1.11.16",
		"laminas/laminas-router" : "^3.5"
	},
	"require-dev" : {
		"phpunit/phpunit" : "8.5.*"
	},
	"suggest" : {
		"krakjoe/pthreads-polyfill" : "^1.1",
		"ext-ds" : "*",
		"ext-pthreads" : "*"
	},
	"autoload" : {
		"psr-4" : {
			"Slothsoft\\Farah\\" : "src"
		},
		"files" : [
			"src/bootstrap.php"
		]
	},
	"keywords" : [
		"slothsoft",
		"farah-module",
		"docs"
	],
	"autoload-dev" : {
		"psr-4" : {
			"Slothsoft\\Farah\\" : "tests"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-main" : "1.14.x-dev"
		}
	},
	"homepage" : "http://farah.slothsoft.net/modules/farah"
}
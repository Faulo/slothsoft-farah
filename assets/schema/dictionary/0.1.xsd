	xmlns:sfd="http://schema.slothsoft.net/farah/dictionary"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	elementFormDefault="qualified" attributeFormDefault="unqualified"
	version="0.1">
	
	<xsd:annotation>
		<xsd:documentation><![CDATA[
Schema    : Farah Dictionary
Version   : 0.1
Revision  : R20180402
Namespace : http://schema.slothsoft.net/farah/dictionary
Author    : Daniel Schulz (daniel.lio.schulz@gmail.com)

*** Version 1.0 ***
	R20180402
		- initial release
		]]></xsd:documentation>
	</xsd:annotation>
	
	<xsd:attributeGroup name="schemaVersion">

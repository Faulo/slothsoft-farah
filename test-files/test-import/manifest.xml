<?xml version="1.0"?>
<assets xmlns="http://schema.slothsoft.net/farah/module" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.1">
	<fragment name="import">
		<custom-asset name="test" executable-builder="Slothsoft\Farah\Module\PrintFragmentExecutableBuilder" />
	</fragment>

	<fragment name="result-import">
		<import ref="/import" />
	</fragment>

	<fragment name="result-use-manifest">
		<use-manifest ref="/import/test" name="test" />
	</fragment>

	<fragment name="result-use-document">
		<use-document ref="/import/test" name="test" />
	</fragment>

	<fragment name="result-use-document-with-hash">
		<use-document ref="/import/test#hash" name="test" />
	</fragment>

	<fragment name="result-use-template">
		<use-template ref="/import/test" name="test" />
	</fragment>

	<fragment name="result-link-stylesheet">
		<link-stylesheet ref="/import/test" name="test" />
	</fragment>

	<fragment name="result-link-script">
		<link-script ref="/import/test" name="test" />
	</fragment>
</assets>

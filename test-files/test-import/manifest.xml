<?xml version="1.0"?>
<assets xmlns="http://schema.slothsoft.net/farah/module" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.1">
	<fragment name="import">
		<custom-asset name="fragment" executable-builder="Slothsoft\Farah\Module\PrintFragmentExecutableBuilder" />
		<custom-asset name="query" executable-builder="Slothsoft\Farah\Module\PrintQueryExecutableBuilder" />
	</fragment>

	<fragment name="result-import">
		<import ref="/import" />
	</fragment>

	<fragment name="result-use-manifest">
		<use-manifest ref="/import/fragment" name="fragment" />
	</fragment>

	<fragment name="result-use-document">
		<use-document ref="/import/fragment" name="fragment" />
	</fragment>

	<fragment name="result-use-document-no-name">
		<use-document ref="/import/fragment" />
	</fragment>

	<fragment name="result-use-document-no-name-traversal">
		<use-document ref="../../import/query/../fragment/." />
	</fragment>

	<fragment name="result-use-document-no-name-root">
		<use-document ref="/" />
	</fragment>

	<fragment name="result-use-document-no-name-farah">
		<use-document ref="//slothsoft@farah" />
	</fragment>

	<fragment name="result-use-document-with-hash">
		<use-document ref="/import/fragment#hash" name="fragment" />
	</fragment>

	<fragment name="result-use-document-with-query">
		<use-document ref="/import/query?a=1&amp;c" name="query" />
	</fragment>

	<fragment name="result-use-template">
		<use-template ref="/import/fragment" name="fragment" />
	</fragment>

	<fragment name="result-link-stylesheet">
		<link-stylesheet ref="/import/fragment" name="fragment" />
	</fragment>

	<fragment name="result-link-script">
		<link-script ref="/import/fragment" name="fragment" />
	</fragment>
</assets>
